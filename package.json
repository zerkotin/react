{
  "name": "react-stack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zerkotin/react.git"
  },
  "scripts": {
    "lint": "eslint \"src/js/**\" -o build/eslint.html -f html",
    "snapshot": "jest --updateSnapshot",
    "test": "jest",
    "watch": "webpack -d --watch",
    "build": "webpack -p",
    "start": "cd build && http-server -c-1"
  },
  "author": "Itzik Cohen Arazi",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "src/test/js/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "globals": {
      "widow": true
    },
    "setupFiles": [
      "<rootDir>/src/test/js/browserMocks.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^0.28.11",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "radium": "^0.24.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.5.0",
    "redux": "^4.0.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2"
  },
  "devDependencies": {
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "webpack-cli": "^3.1.0"
  }
}
